                                <th colspan="4" data-i18n="ci.repo"></th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr id="url">
                                <td>
                                    <input name="svnUrl" type="text" data-i18n="[placeholder]ci.placeholder.svnurl">
