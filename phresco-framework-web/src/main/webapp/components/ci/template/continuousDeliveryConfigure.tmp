                            		<input type="radio" name="scheduleType" value="Weekly"><span data-i18n="ci.weekly"></span>
                            		<input type="radio" name="scheduleType" value="Monthly"><span data-i18n="ci.monthly"></span>
                            	</td>
                            </tr>
                        </tbody>
                    </table>
