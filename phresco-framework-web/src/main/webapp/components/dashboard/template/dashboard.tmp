                                        <select id="data_type">
                                        	<option>Splunk</option>
                                        </select>
                                    </td>
                    			</tr>
                                <tr>
	                                <td>
                                    	<label>Username<sup>*</sup></label>
                                    	<input type="text" id="conf_username">
