<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<mojos>
    <mojo>
		<goal>load-test</goal>
		<description>Goal which load Test to the Java QuickStart project</description>
		<requiresDirectInvocation>false</requiresDirectInvocation>
		<requiresProject>true</requiresProject>
		<requiresReports>false</requiresReports>
		<aggregator>false</aggregator>
		<requiresOnline>false</requiresOnline>
		<inheritedByDefault>true</inheritedByDefault>
		<language>java</language>
		<instantiationStrategy>per-lookup</instantiationStrategy>
		<executionStrategy>once-per-session</executionStrategy>
		<threadSafe>false</threadSafe>
			<configuration>
			<parameters>
				<parameter>
                    <name>
                        <value lang="en">Test Against</value>
                    </name>
                    <type>List</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>testAgainst</key>
                    <possibleValues>
                        <value key="server" dependency="loadContextUrl">Server</value>
                        <value key="webservice" dependency="loadContextUrl">Web Services</value>
                    </possibleValues>
                    <multiple>false</multiple>
                    <value></value>
                    <sort>false</sort>
                    <show>true</show>
                    <dependency>configurations,testName</dependency>
                </parameter>
				<parameter>
					<name>
						<value lang="en">Show Settings</value>
					</name>
					<type>Boolean</type>
					<required>false</required>
					<editable>true</editable>
					<description></description>
					<key>showSettings</key>
					<multiple>false</multiple>
					<value>false</value>
					<sort>false</sort>
					<show>true</show>
					<dependency>environmentName</dependency>
				</parameter>
				<parameter>
					<name>
						<value lang="en">Environment</value>
					</name>
					<type>DynamicParameter</type>
					<dynamicParameter>
						<class>com.photon.phresco.impl.EnvironmentsParameterImpl</class>
						<dependencies>
							<dependency>
								<groupId>com.photon.phresco.commons</groupId>
								<artifactId>phresco-commons</artifactId>
								<type>jar</type>
								<version>3.3.1.3000-SNAPSHOT</version>
							</dependency>
						</dependencies>
					</dynamicParameter>
					<required>true</required>
					<editable>true</editable>
					<key>environmentName</key>
					<multiple>false</multiple>
					<value></value>
					<sort>false</sort>
					<show>true</show>
					<dependency>configurations</dependency>
				</parameter>
				<parameter>
                   <name>
                       <value lang="en">Configurations</value>
                   </name>
                   <type>DynamicParameter</type>
                   <dynamicParameter>
                       <class>com.photon.phresco.framework.param.impl.PerformanceConfigurationsImpl</class>
						<dependencies>
                           <dependency>
								<groupId>com.photon.phresco.framework</groupId>
								<artifactId>phresco-framework-impl</artifactId>
								<type>jar</type>
								<version>3.3.1.3000-SNAPSHOT</version>
                           </dependency>
                       </dependencies>
                   </dynamicParameter>
                   <required>false</required>
                   <editable>true</editable>
                   <description></description>
                   <key>configurations</key>
                   <multiple>false</multiple>
                   <value></value>
                   <sort>false</sort>
                   <show>true</show>
                </parameter>
				<parameter>
                    <name>
                        <value lang="en">Test Result Name</value>
                    </name>
                    <type>DynamicParameter</type>
                    <dynamicParameter>
                        <class>com.photon.phresco.framework.param.impl.PerformanceTestResultNamesImpl</class>
                        <dependencies>
                            <dependency>
								<groupId>com.photon.phresco.framework</groupId>
								<artifactId>phresco-framework-impl</artifactId>
								<type>jar</type>
								<version>3.3.1.3000-SNAPSHOT</version>
                            </dependency>
                        </dependencies>
                    </dynamicParameter>
                    <required>true</required>
                    <editable>edit</editable>
                    <description></description>
                    <key>testName</key>
                    <multiple>false</multiple>
                    <value></value>
                    <sort>false</sort>
                    <show>true</show>
                </parameter>
				<parameter>
					<name>
						<value lang="en">No Of Users</value>
					</name>
					<type>Number</type>
					<required>true</required>
					<editable>true</editable>
					<description></description>
					<key>noOfUsers</key>
					<multiple>false</multiple>
					<value>10</value>
					<sort>false</sort>
					<show>true</show>
				</parameter>
				<parameter>
					<name>
						<value lang="en">Ramp-Up Period</value>
					</name>
					<type>Number</type>
					<required>true</required>
					<editable>true</editable>
					<description></description>
					<key>rampUpPeriod</key>
					<multiple>false</multiple>
					<value>10</value>
					<sort>false</sort>
					<show>true</show>
				</parameter>
				<parameter>
					<name>
						<value lang="en">Loop Count</value>
					</name>
					<type>Number</type>
					<required>true</required>
					<editable>true</editable>
					<description></description>
					<key>loopCount</key>
					<multiple>false</multiple>
					<value>10</value>
					<sort>false</sort>
					<show>true</show>
				</parameter>
				<parameter>
                    <name>
                        <value lang="en">Context URLs</value>
                    </name>
                    <type>DynamicPageParameter</type>
                    <dynamicParameter>
                        <class>com.photon.phresco.framework.param.impl.LoadTestDetailsImpl</class>
                        <dependencies>
                            <dependency>
								<groupId>com.photon.phresco.framework</groupId>
								<artifactId>phresco-framework-impl</artifactId>
								<type>jar</type>
								<version>3.3.1.3000-SNAPSHOT</version>
                            </dependency>
                        </dependencies>
                    </dynamicParameter>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>loadContextUrl</key>
                    <multiple>false</multiple>
                    <value></value>
                    <sort>false</sort>
                    <show>false</show>
                </parameter>
				 <parameter>
                    <pluginParameter>framework</pluginParameter>
                    <mavenCommands>
                        <mavenCommand>
                            <key>showErrors</key>
                            <value>-e</value>
                        </mavenCommand>
                        <mavenCommand>
                            <key>hideLogs</key>
                            <value>-q</value>
                        </mavenCommand>
                        <mavenCommand>
                            <key>showDebug</key>
                            <value>-X</value>
                        </mavenCommand>
                    </mavenCommands>
                    <name>
                        <value lang="en">Logs</value>
                    </name>
                    <type>List</type>
                    <required>false</required>
                    <editable>true</editable>
                    <key>logs</key>
                    <possibleValues>
                        <value key="showErrors">Show Errors</value>
                        <value key="hideLogs">Hide Logs</value>
                        <value key="showDebug">Show Debug</value>
                    </possibleValues>
                    <multiple>false</multiple>
                    <value>showErrors</value>
                    <sort>false</sort>
                    <show>true</show>
                </parameter>
			</parameters>
		</configuration>
	</mojo>
</mojos>