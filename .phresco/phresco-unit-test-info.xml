<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<mojos>
    <mojo>
        <goal>unit-test</goal>
        <description>Goal which packages the HTML5-Jquery project</description>
        <requiresDirectInvocation>false</requiresDirectInvocation>
        <requiresProject>true</requiresProject>
        <requiresReports>false</requiresReports>
        <aggregator>false</aggregator>
        <requiresOnline>false</requiresOnline>
        <inheritedByDefault>true</inheritedByDefault>
        <implementation>
            <dependency>
                <groupId>com.photon.phresco.plugins</groupId>
                <artifactId>java-phresco-plugin</artifactId>
                <type>jar</type>
                <version>3.3.0.5001-SNAPSHOT</version>
                <class>com.photon.phresco.plugins.java.JavaPlugin</class>
            </dependency>
        </implementation>
        <language>java</language>
        <instantiationStrategy>per-lookup</instantiationStrategy>
        <executionStrategy>once-per-session</executionStrategy>
        <threadSafe>false</threadSafe>
        <configuration>
            <parameters>
                <parameter>
                    <pluginParameter>plugin</pluginParameter>
                    <mavenCommands>
                        <mavenCommand>
                            <key>java</key>
                            <value>-Pjava</value>
                        </mavenCommand>
                        <mavenCommand>
                            <key>js</key>
                            <value>-Pjs -DskipTests</value>
                        </mavenCommand>
                    </mavenCommands>
                    <name>
                        <value lang="en">Test Against</value>
                    </name>
                    <type>List</type>
                    <required>true</required>
                    <editable>true</editable>
                    <key>testAgainst</key>
                    <possibleValues>
                        <value key="java">java</value>
                        <value key="js" dependency="showSettings,environmentName">js</value>
                    </possibleValues>
                    <multiple>false</multiple>
                    <value></value>
                    <sort>false</sort>
                    <show>true</show>
                </parameter>
				 <parameter>
                    <name>
                        <value lang="en">Show Settings</value>
                    </name>
                    <type>Boolean</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>showSettings</key>
                    <multiple>false</multiple>
                    <value></value>
                    <sort>false</sort>
                    <show>false</show>
                    <dependency>environmentName</dependency>
                </parameter>
				  <parameter>
                    <name>
                        <value lang="en">Environment</value>
                    </name>
                    <type>DynamicParameter</type>
                    <dynamicParameter>
                        <class>com.photon.phresco.impl.EnvironmentsParameterImpl</class>
                        <dependencies>
                            <dependency>
								<groupId>com.photon.phresco.commons</groupId>
								<artifactId>phresco-commons</artifactId>
								<type>jar</type>
								<version>3.3.0.5001-SNAPSHOT</version>
                            </dependency>
                        </dependencies>
                    </dynamicParameter>
                    <required>true</required>
                    <editable>true</editable>
                    <key>environmentName</key>
                    <multiple>false</multiple>
                    <value></value>
                    <sort>false</sort>
                    <show>true</show>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Module</value>
                    </name>
                    <type>Hidden</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>projectModule</key>
                    <multiple>false</multiple>
                    <value>phresco-framework</value>
                    <sort>false</sort>
                    <show>false</show>
                </parameter>
            </parameters>
        </configuration>
    </mojo>
</mojos>
