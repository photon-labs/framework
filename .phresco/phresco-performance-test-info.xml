<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<mojos>
    <mojo>
        <goal>performance-test</goal>
        <description>Goal which execute the jMeter performance test</description>
        <requiresDirectInvocation>false</requiresDirectInvocation>
        <requiresProject>true</requiresProject>
        <requiresReports>false</requiresReports>
        <aggregator>false</aggregator>
        <requiresOnline>false</requiresOnline>
        <inheritedByDefault>true</inheritedByDefault>
        <language>java</language>
        <instantiationStrategy>per-lookup</instantiationStrategy>
        <executionStrategy>once-per-session</executionStrategy>
        <threadSafe>false</threadSafe>
        <configuration>
            <parameters>
                <parameter>
                    <name>
                        <value lang="en">Test Against</value>
                    </name>
                    <type>List</type>
                    <required>true</required>
                    <editable>true</editable>
                    <description></description>
                    <key>testAgainst</key>
                    <possibleValues>
                        <value key="server" dependency="contextUrls">Server</value>
                        <value key="webservice" dependency="contextUrls">Web Services</value>
                        <value key="database" dependency="dbContextUrls">Database</value>
                    </possibleValues>
                    <multiple>false</multiple>
                    <value>server</value>
                    <sort>false</sort>
                    <show>true</show>
                    <dependency>configurations,testName</dependency>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Show Settings</value>
                    </name>
                    <type>Boolean</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>showSettings</key>
                    <multiple>false</multiple>
                    <value>false</value>
                    <sort>false</sort>
                    <show>true</show>
                    <dependency>environmentName</dependency>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Environment</value>
                    </name>
                    <type>DynamicParameter</type>
                    <dynamicParameter>
                        <class>com.photon.phresco.impl.EnvironmentsParameterImpl</class>
                        <dependencies>
                            <dependency>
<groupId>com.photon.phresco.commons</groupId>
<artifactId>phresco-commons</artifactId>
<type>jar</type>
<version>2.3.0.4001</version>
                            </dependency>
                        </dependencies>
                    </dynamicParameter>
                    <required>true</required>
                    <editable>true</editable>
                    <key>environmentName</key>
                    <multiple>false</multiple>
                    <value>Production</value>
                    <sort>false</sort>
                    <show>true</show>
                    <dependency>configurations</dependency>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Configurations</value>
                    </name>
                    <type>DynamicParameter</type>
                    <dynamicParameter>
                        <class>com.photon.phresco.framework.param.impl.PerformanceConfigurationsImpl</class>
                        <dependencies>
                            <dependency>
<groupId>com.photon.phresco.framework</groupId>
<artifactId>phresco-framework-impl</artifactId>
<type>jar</type>
<version>2.3.0.4001</version>
                            </dependency>
                        </dependencies>
                    </dynamicParameter>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>configurations</key>
                    <multiple>false</multiple>
                    <value>server</value>
                    <sort>false</sort>
                    <show>true</show>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Test Result Name</value>
                    </name>
                    <type>DynamicParameter</type>
                    <dynamicParameter>
                        <class>com.photon.phresco.framework.param.impl.PerformanceTestResultNamesImpl</class>
                        <dependencies>
                            <dependency>
<groupId>com.photon.phresco.framework</groupId>
<artifactId>phresco-framework-impl</artifactId>
<type>jar</type>
<version>2.3.0.4001</version>
                            </dependency>
                        </dependencies>
                    </dynamicParameter>
                    <required>true</required>
                    <editable>edit</editable>
                    <description></description>
                    <key>testName</key>
                    <multiple>false</multiple>
                    <value>PerfTest</value>
                    <sort>false</sort>
                    <show>true</show>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">No of Users</value>
                    </name>
                    <type>Number</type>
                    <required>true</required>
                    <editable>true</editable>
                    <description></description>
                    <key>noOfUsers</key>
                    <multiple>false</multiple>
                    <value>5</value>
                    <sort>false</sort>
                    <show>true</show>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Ramp-Up Period</value>
                    </name>
                    <type>Number</type>
                    <required>true</required>
                    <editable>true</editable>
                    <description></description>
                    <key>rampUpPeriod</key>
                    <multiple>false</multiple>
                    <value>1</value>
                    <sort>false</sort>
                    <show>true</show>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Loop Count</value>
                    </name>
                    <type>Number</type>
                    <required>true</required>
                    <editable>true</editable>
                    <description></description>
                    <key>loopCount</key>
                    <multiple>false</multiple>
                    <value>1</value>
                    <sort>false</sort>
                    <show>true</show>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Context URLs</value>
                    </name>
                    <type>DynamicPageParameter</type>
                    <dynamicParameter>
                        <class>com.photon.phresco.framework.param.impl.PerformanceTestDetailsImpl</class>
                        <dependencies>
                            <dependency>
<groupId>com.photon.phresco.framework</groupId>
<artifactId>phresco-framework-impl</artifactId>
<type>jar</type>
<version>2.3.0.4001</version>
                            </dependency>
                        </dependencies>
                    </dynamicParameter>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>contextUrls</key>
                    <multiple>false</multiple>
                    <value></value>
                    <sort>false</sort>
                    <show>false</show>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">DB Context URLs</value>
                    </name>
                    <type>DynamicPageParameter</type>
                    <dynamicParameter>
                        <class>com.photon.phresco.framework.param.impl.PerformanceTestDetailsImpl</class>
                        <dependencies>
                            <dependency>
<groupId>com.photon.phresco.framework</groupId>
<artifactId>phresco-framework-impl</artifactId>
<type>jar</type>
<version>2.3.0.4001</version>
                            </dependency>
                        </dependencies>
                    </dynamicParameter>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>dbContextUrls</key>
                    <multiple>false</multiple>
                    <value></value>
                    <sort>false</sort>
                    <show>false</show>
                </parameter>
            </parameters>
        </configuration>
    </mojo>
</mojos>
